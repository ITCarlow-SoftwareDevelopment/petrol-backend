<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
         version="2.5">

    <servlet>
      <servlet-name>createfillup</servlet-name>
      <servlet-class>com.chris.petrolapp.basicactions.CreateFillUpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>createfillup</servlet-name>
      <url-pattern>/createfillup</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>listfillup</servlet-name>
      <servlet-class>com.chris.petrolapp.basicactions.ListFillUpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>listfillup</servlet-name>
      <url-pattern>/fillups</url-pattern>
    </servlet-mapping>

</web-app>
